{
  "manifest_version": 2,
  "name": "URL Converter",
  "description": "Converts raw.githubusercontent.com URLs to github.com URLs",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "URL Converter",
    "default_popup": "index.html"
  },
  "permissions": ["activeTab"]
}